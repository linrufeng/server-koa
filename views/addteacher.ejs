<%- include('./include/header')%>
    <title><%= title%></title>
</head>
<body>
    <div class="h2box">
       <h2><%= title%></h2>
    </div>
    
    <ul>
        <% list.forEach(function(item){ %>
          <li>
            <a href="/edit?id=<%=item._id %>">去编辑</a>
            <a href="/addteacher?id=<%=item._id %>">去添加教师</a>
           <p><%= item.fullname %> </p> 
           <P><%= item.name %></P> 
           <P><%= item.msg %></P> 
           <P><%= item.user %></P> 
          </li>      
        <% }); %>
      </ul>
    <script src="../jq.js"></script>
    <script>
        $("#login").on('click', function () {
           
            var datas = $('#formDate').serializeArray()           
            var temp = {};
            datas.forEach((item)=>{
                temp[item.name] = item.value;
            })
            $.ajax({
              url: '/eduAdd',
              method:'post',
              data: temp,
              success: function (res) {
                console.log(res)
              },
              error: function(err) {
                  console.log(err)
              }
            })
        })
    $('#getdata').click(function(){
        $.ajax({
              url: '/eduList',
              method:'get',             
              success: function (res) {
                console.log(res)
              },
              error: function(err) {
                  console.log(err)
              }
            })
    })
    </script>
</body>
</html>